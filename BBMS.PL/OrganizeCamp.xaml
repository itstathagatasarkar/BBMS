        <Label Content="City" HorizontalAlignment="Left" Margin="18,198,0,0" VerticalAlignment="Top" Width="81"/>
        <Label Content="Blood Bank" HorizontalAlignment="Left" Margin="18,239,0,0" VerticalAlignment="Top" Width="81" RenderTransformOrigin="0.393,1.737"/>
        <Label Content="Start Date" HorizontalAlignment="Left" Margin="18,285,0,0" VerticalAlignment="Top" Width="81"/>
        <Label Content="End Date" HorizontalAlignment="Left" Margin="18,325,0,0" VerticalAlignment="Top" Width="81"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="232,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="232,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
